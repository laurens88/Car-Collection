<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UpdateActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="-19dp">

    <EditText
        android:id="@+id/editBrand2"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Brand"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.055"
        android:backgroundTint="@color/black_overlay"/>

    <EditText
        android:id="@+id/editType2"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Type"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.932"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.055"
        android:backgroundTint="@color/black_overlay"/>

    <EditText
        android:id="@+id/editPlate2"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/black_overlay"
        android:digits="ABCDEFGHJKLMNOPQRSTVWXYZ-0123456789"
        android:ems="10"
        android:hint="License Plate"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.736" />


    <Button
        android:id="@+id/cancel_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/red"
        android:text="Cancel"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/editPlate2"
        app:layout_constraintHorizontal_bias="0.679"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.736" />

    <EditText
        android:id="@+id/editEdition"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Edition"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.18"
        android:backgroundTint="@color/black_overlay" />

    <EditText
        android:id="@+id/editPrice2"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Price"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:digits="â‚¬0123456789.-"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.932"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.18"
        android:backgroundTint="@color/black_overlay"/>

    <EditText
        android:id="@+id/editColor2"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Color"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.304"
        android:backgroundTint="@color/black_overlay"/>

    <EditText
        android:id="@+id/editYear2"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Year"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:digits="0123456789"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.936"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.304"
        android:backgroundTint="@color/black_overlay"/>

    <EditText
        android:id="@+id/editAcceleration"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="0-100"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:digits="0123456789. sec"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.425"
        android:backgroundTint="@color/black_overlay"/>

    <EditText
        android:id="@+id/editTopspeed"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Top speed"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:digits="0123456789 km/hu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.932"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.425"
        android:backgroundTint="@color/black_overlay"/>


    <EditText
        android:id="@+id/editPower2"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Power"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:digits="0123456789 pkhPKH"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.063"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.54"
        android:backgroundTint="@color/black_overlay"/>

    <EditText
        android:id="@+id/editNote"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:backgroundTint="@color/black_overlay"
        android:digits="0123456789 abcdefghijklmnopqrstuvwxyz!?.,/ABCDEFGHIJKLMNOPQRSTUVWXYZ+"
        android:maxLength="20"
        android:ems="10"
        android:hint="Note"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.063"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65" />

    <EditText
        android:id="@+id/editRanking"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Ranking"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:digits="0123456789"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.932"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.54"
        android:backgroundTint="@color/black_overlay"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Brand"
        app:layout_constraintBottom_toTopOf="@+id/editBrand2"
        app:layout_constraintStart_toStartOf="@+id/editBrand2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Type"
        app:layout_constraintBottom_toTopOf="@+id/editType2"
        app:layout_constraintStart_toStartOf="@+id/editType2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Price"
        app:layout_constraintBottom_toTopOf="@+id/editPrice2"
        app:layout_constraintStart_toStartOf="@+id/editPrice2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Power"
        app:layout_constraintBottom_toTopOf="@+id/editPower2"
        app:layout_constraintStart_toStartOf="@+id/editPower2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:text="Color"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editEdition" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Year"
        app:layout_constraintBottom_toTopOf="@+id/editYear2"
        app:layout_constraintStart_toStartOf="@+id/editYear2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0-100 in"
        app:layout_constraintBottom_toTopOf="@+id/editAcceleration"
        app:layout_constraintStart_toStartOf="@+id/editAcceleration" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Top speed"
        app:layout_constraintBottom_toTopOf="@+id/editTopspeed"
        app:layout_constraintStart_toStartOf="@+id/editTopspeed" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ranking"
        app:layout_constraintBottom_toTopOf="@+id/editRanking"
        app:layout_constraintStart_toStartOf="@+id/editRanking" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Edition"
        app:layout_constraintBottom_toTopOf="@+id/editEdition"
        app:layout_constraintStart_toStartOf="@+id/editEdition" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="178dp"
        android:layout_height="44dp"
        android:minHeight="48dp"
        android:spinnerMode="dropdown"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.303" />

    <TextView
        android:id="@+id/noteTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Note"
        app:layout_constraintBottom_toTopOf="@+id/editNote"
        app:layout_constraintStart_toStartOf="@+id/editNote" />

    <include
        android:id="@+id/update_button"
        layout="@layout/update_button"
        android:layout_width="99dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.91"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.735" />


</androidx.constraintlayout.widget.ConstraintLayout>